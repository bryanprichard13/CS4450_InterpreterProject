Grammar

    0 $accept: statement $end

    1 statement: assign
    2          | exp

    3 assign: TOKEN_STRING '=' exp
    4       | TOKEN_STRING "+=" exp
    5       | TOKEN_STRING "-=" exp
    6       | TOKEN_STRING "*=" exp
    7       | TOKEN_STRING "/=" exp
    8       | TOKEN_STRING "^=" exp
    9       | TOKEN_STRING "%=" exp

   10 exp: '(' exp ')'
   11    | TOKEN_INT
   12    | TOKEN_FLOAT
   13    | TOKEN_STRING
   14    | exp '+' exp
   15    | exp '-' exp
   16    | exp '*' exp
   17    | exp '/' exp
   18    | exp '%' exp
   19    | exp '^' exp
   20    | '-' exp


